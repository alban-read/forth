25 value bench_times

: statement
  ." Running benchmarks " bench_times . ." times" cr ;

statement

fload bench.fib
fload bench.sieve
fload bench.matrix
fload bench.graphics


cr .( running Fib )
run-fib
cr .( running Sieve )
run-sieve
cr .( running Matrix )
run-matrix
cr .( running Graphics )
run-graphics-1

